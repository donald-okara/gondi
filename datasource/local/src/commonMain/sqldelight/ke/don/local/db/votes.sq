CREATE TABLE voteEntity (
    voter_id TEXT NOT NULL,
    target_id TEXT NOT NULL,
    is_guilty INTEGER NOT NULL,
    PRIMARY KEY (voter_id)
);

getAllVotes:
SELECT * FROM voteEntity;

insertOrReplaceVote:
INSERT OR REPLACE INTO voteEntity(voter_id, target_id, is_guilty)
VALUES (:voter_id, :target_id, :is_guilty);

clearVotes:
DELETE FROM voteEntity;
